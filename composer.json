        "ckeditor/mentions": "4.17.2",
        "ckeditor/notification": "4.17.2",
        "ckeditor/notificationaggregator": "4.17.2",
        "ckeditor/textmatch": "4.17.2",
        "ckeditor/textwatcher": "4.17.2",
        "ckeditor/xml": "4.17.2",
        "composer/installers": "^1.2",
